#include <iostream>
using namespace std;

string warnaLampu(int t) {
    int cycle = 103; // total durasi satu siklus
    int rel = (t - 45) % cycle;
    if (rel < 0) rel += cycle; // jaga agar tidak negatif

    if (rel <= 2) return "Kuning";
    else if (rel <= 82) return "Merah";
    else return "Hijau";
}

int main() {
    int times[] = {80, 135, 150, 212};
    for (int t : times) {
        cout << "Detik " << t << " = " << warnaLampu(t) << endl;
    }
    return 0;
}
